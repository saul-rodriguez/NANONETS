; lvsLayoutViewList    = '( "msps" "auLvs" "extracted" "schematic" "symbol")
; lvsSchematicStopList = '( "msps" "auLvs" "symbol")
; lvsLayoutStopList    = '( "msps" "auLvs" "symbol")

; For non Analog Artist (DFII) LVS comparison 
