  lvsSchematicViewList = '( "auLvs" "schematic" "cmos_sch" "netlist" "symbol" )
  lvsLayoutViewList    = '( "auLvs" "extracted" "schematic" "symbol")
  lvsSchematicStopList = '( "auLvs" )
  lvsLayoutStopList    = '( "auLvs" )

; For Analog Artist macro-cell comparison 
