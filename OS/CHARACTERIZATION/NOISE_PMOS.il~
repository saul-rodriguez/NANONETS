simulator( 'spectre )
design(	 "/afs/kth.se/home/s/a/saul/Sim/NOISE_PMOS/spectre/schematic/netlist/netlist")
resultsDir( "/afs/kth.se/home/s/a/saul/Sim/NOISE_PMOS/spectre/schematic" )
modelFile( 
    '("/afs/ict.kth.se/proj/ektlab/PDK/AMS/spectre/h18/soac/mcparams.scs" "")
    '("/afs/ict.kth.se/proj/ektlab/PDK/AMS/spectre/h18/soac/design.scs" "noitm")
    '("/afs/ict.kth.se/proj/ektlab/PDK/AMS/spectre/h18/soac/res.scs" "restm")
    '("/afs/ict.kth.se/proj/ektlab/PDK/AMS/spectre/h18/soac/cap.scs" "captm")
    '("/afs/ict.kth.se/proj/ektlab/PDK/AMS/spectre/h18/soac/bip.scs" "biptm")
    '("/afs/ict.kth.se/proj/ektlab/PDK/AMS/spectre/h18/soac/esddi.scs" "esdditm")
    '("/afs/ict.kth.se/proj/ektlab/PDK/AMS/spectre/h18/soac/cmos53.scs" "cmostm")
)
analysis('dc ?saveOppoint t  )
analysis('noise ?start ".001"  ?stop "10G"  ?dec "10"  
		?oprobe "/IPRB0"  ?iprobe "/V2"  )
desVar(	  "IDS" 1u	)
desVar(	  "L" 1u	)
desVar(	  "VDS" .5	)
desVar(	  "W" 10u	)
envOption(
	'analysisOrder  list("dc" "noise") 
)
temp( 27 ) 
run()
input\ noise\;\ A\ \/\ sqrt\(Hz\) = getData("in" ?result "noise")
plot( input\ noise\;\ A\ \/\ sqrt\(Hz\) ?expr '( "input noise; A / sqrt(Hz)" ) )
